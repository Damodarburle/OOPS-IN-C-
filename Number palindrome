#include<iostream>
using namespace std;

int main() {
    int n,r,sum=0;
    cout<<"Enter the number:"<<endl;
    cin>>n;
    
    int temp=n;
    
  while(n>0){
      r=n%10;
      sum=sum*10+r;
      n=n/10;
  }
  
  if(temp==sum) {
      cout<<"It is a palindrome";
  }
  else {
      cout<<"It is not a palindrome";
  }
  return 0;
}
